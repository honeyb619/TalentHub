@{
    ViewBag.Title = "Index";
}

<header class="major">
    <h2>Register your talent</h2>
</header>
<div class="container WhiteBack">
    <div class="row">
        <div class="4u wufoo">
            <section class="section">
                <fieldset>
                    <legend class="headerlegend">Communication Details</legend>
                    <ul>
                        <li>
                            <fieldset>
                                <legend>Name : </legend>

                                <span>
                                    <label for="FirstNameFileld">First</label>
                                    <input id="FirstNameFileld" name="FirstNameFileld" type="text" class="field text fn" value="" size="8" tabindex="1">
                                </span>
                                <span>
                                    <label for="LastNameField">Last</label>
                                    <input id="LastNameField" name="LastNameField" type="text" class="field text ln" value="" size="14" tabindex="2">
                                </span>
                            </fieldset>
                        </li>



                        <li id="fo1li5" class="notranslate ">
                            <label class="desc" id="title5" for="EmailField">
                                Email
                            </label>
                            <div>
                                <input id="EmailField" name="EmailField" type="email" spellcheck="false" class="field text medium" value="" maxlength="255" tabindex="3">
                            </div>
                        </li>



                        <li id="fo1li6" class="notranslate">
                            <label class="desc" id="title6" for="AlternateEmailField">
                                Alternate Email
                            </label>
                            <div>
                                <input id="AlternateEmailField" name="AlternateEmailField" type="email" spellcheck="false" class="field text medium" value="" maxlength="255" tabindex="4">
                            </div>
                        </li>
                        <li id="fo1li14" class="notranslate      ">
                            <label class="desc" id="title14" for="MobileField">
                                Mobile
                            </label>
                            <div>
                                <input id="MobileField" class="field text medium" name="MobileField" tabindex="5" type="text" maxlength="255" value="">
                            </div>
                        </li>



                        <li id="fo1li15" class="notranslate      ">
                            <label class="desc" id="title15" for="PhoneField">
                                Phone Number
                            </label>
                            <div>
                                <input id="PhoneField" class="field text medium" name="PhoneField" tabindex="6" type="text" maxlength="255" value="">
                            </div>
                        </li>



                        <li id="fo1li16" class="complex notranslate      ">
                            <fieldset>
                                <legend>Postal Address : </legend>
                                <span class="full addr1">
                                    <label for="StAddressField">Street Address</label>
                                    <input id="StAddressField" name="StAddressField" type="text" class="field text addr" value="" tabindex="7">
                                </span>
                                <span class="full addr2">
                                    <label for="Addline1Field">Address Line 2</label>
                                    <input id="Addline1Field" name="Addline1Field" type="text" class="field text addr" value="" tabindex="8">
                                </span>
                                <span class="left city">
                                    <label for="CityField">City</label>
                                    <input id="CityField" name="CityField" type="text" class="field text addr" value="" tabindex="9">
                                </span>
                                <span class="right state">
                                    <label for="StateField">State / Province / Region</label>
                                    <input id="StateField" name="StateField" type="text" class="field text addr" value="" tabindex="10">
                                </span>
                                <span class="left zip">
                                    <label for="PostalField">Postal / Zip Code</label>
                                    <input id="PostalField" name="PostalField" type="text" class="field text addr" value="" maxlength="15" tabindex="11">
                                </span>
                                <span class="right country">
                                    <label for="CountryField">Country</label>
                                    <select id="CountryField" name="CountryField" class="field select addr" tabindex="12">
                                        <option value="" selected="selected">Please Select</option>

                                    </select>

                                </span>
                            </fieldset>
                        </li>
                    </ul>
                </fieldset>

            </section>
        </div>
        <div class="4u wufoo">
            <section class="section">
                <fieldset>
                    <legend class="headerlegend">Personal Details</legend>
                    <ul>
                        <li>
                            <fieldset>
                                <legend>Appearance : </legend>
                                <span>
                                    <label for="AgeField">Age</label>
                                    <input id="AgeField" name="AgeField" type="text" class="field text fn" value="" size="8" tabindex="13">
                                </span>
                                <span>
                                    <label for="HeightField">Height</label>
                                    <input id="HeightField" name="HeightField" type="text" class="field text ln" value="" size="14" tabindex="14">
                                </span>
                                <span>
                                    <label for="ChestSizeField">Chest/Breast Size</label>
                                    <input id="ChestSizeField" name="ChestSizeField" type="text" class="field text ln" value="" size="14" tabindex="15">
                                </span>
                                <span>
                                    <label for="WaistField">Waist</label>
                                    <input id="WaistField" name="WaistField" type="text" class="field text ln" value="" size="14" tabindex="16">
                                </span>
                                <span>
                                    <label for="HipSizeField">Hip Size</label>
                                    <input id="HipSizeField" name="HipSizeField" type="text" class="field text ln" value="" size="14" tabindex="17">
                                </span>
                                <span>
                                    <label for="EyeColorField">Eye Color</label>
                                    <input id="EyeColorField" name="EyeColorField" type="text" class="field text ln" value="" size="14" tabindex="18">
                                </span>

                                <span>
                                    <label for="HairColorField">Hair Color</label>
                                    <input id="HairColorField" name="HairColorField" type="text" class="field text ln" value="" size="14" tabindex="19">
                                </span>

                            </fieldset>
                            <span>
                                <label for="HobbiesField">Hobbies</label>
                                <textarea id="HobbiesField" name="HobbiesField" class="field text ln" tabindex="20">
                                    </textarea>
                            </span>
                            <span>
                                <label for="skillsField">Skills</label>
                                <textarea id="skillsField" name="skillsField" class="field text ln" tabindex="21">
                                    </textarea>
                            </span>
                            <span>
                                <label for="AcsentField">Acsent</label>
                                <input id="AcsentField" name="AcsentField" type="text" class="field text ln" value="" size="14" tabindex="22">
                            </span>
                            <span>
                                <label for="NationalityField">Nationality</label>
                                <input id="NationalityField" name="NationalityField" type="text" class="field text ln" value="" size="14" tabindex="23">
                            </span>
                            <span>
                                <label for="RegionField">Region</label>
                                <select id="RegionField" name="RegionField" class="field select addr" tabindex="24">
                                    <option value="" selected="selected">Please Select</option>

                                </select>
                            </span>
                            <span>
                                <label for="EthicityField">Ethicity</label>
                                <input id="EthicityField" name="EthicityField" type="text" class="field text ln" value="" size="14" tabindex="25">
                            </span>


                        </li>
                    </ul>
                </fieldset>


            </section>
        </div>
        <div class="4u wufoo">
            <section class="section">
                <div>
                    <fieldset>
                        <legend class="headerlegend">Talent Details</legend>
                        <ul>
                            <li>
                                <fieldset>
                                    <legend>Talent 1 : </legend>
                                    <span>
                                        <label for="skill1NameField">Area of Expertise</label>
                                        <select id="skill1NameField" name="skill1NameField" class="field select addr" tabindex="26">
                                            <option value="" selected="selected">Please Select</option>

                                        </select>
                                    </span>
                                    <span>
                                        <label for="skill1DescField">Description</label>
                                        <textarea id="skill1DescField" name="skill1DescField" class="field text ln" tabindex="27">
                                            </textarea>
                                    </span>

                                </fieldset>
                                <fieldset>
                                    <legend>Talent 2 : </legend>
                                    <span>
                                        <label for="skill2NameField">Area of Expertise</label>
                                        <select id="skill2NameField" name="skill2NameField" class="field select addr" tabindex="28">
                                            <option value="" selected="selected">Please Select</option>

                                        </select>
                                    </span>
                                    <span>
                                        <label for="skill2DescField">Description</label>
                                        <textarea id="skill2DescField" name="skill2DescField" class="field text ln" tabindex="29">
                                            </textarea>

                                    </span>

                                </fieldset>
                                <fieldset>
                                    <legend>Talent 3 : </legend>
                                    <span>
                                        <label for="skill3NameField">Area of Expertise</label>
                                       <select id="skill3NameField" name="skill3NameField" class="field select addr" tabindex="30">
                                            <option value="" selected="selected">Please Select</option>

                                        </select>
                                    </span>
                                    <span>
                                        <label for="skill3DescField">Description</label>
                                        <textarea id="skill3DescField" name="skill3DescField" class="field text ln" tabindex="31">
                                            </textarea>
                                    </span>

                                </fieldset>
                            </li>
                            <li>

                                <span>
                                    <input id="profilePublicField" name="profilePublicField" type="checkbox" class="field text fn" tabindex="32">
                                    <label for="profilePublicField" style="font-size: 0.9em;" title="The Profile will be visible at jade talent website.">Do you want your profile to be public?</label>

                                </span>

                            </li>
                            <li>
                                <input type="submit" value="SUBMIT" tabindex="33" />
                            </li>
                        </ul>
                    </fieldset>
                </div>
            </section>
        </div>
    </div>
</div>

